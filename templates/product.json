        "custom_liquid": "{% assign video = product.metafields.custom.product_video.value %}\n{% if video %}\n  <style>.embed-container{position:relative;padding-bottom:56.25%;height:0;overflow:hidden;max-width:1200px;margin:16px auto 0}.embed-container iframe,.embed-container object,.embed-container embed{position:absolute;top:0;left:0;width:100%;height:100%}</style>\n  <div class=\"custom-section product-video\">\n    <div class=\"embed-container\">\n      {% if video.source == 'YouTube' %}\n        <script\n          type=\"module\"\n          src=\"https://cdn.jsdelivr.net/npm/@justinribeiro/lite-youtube@1/lite-youtube.min.js\"\n        ></script>\n        {% assign params = video.product_video_url | split: 'v=' %}\n        <lite-youtube videoid=\"{{params[1]}}\" params=\"rel=0\">\n          <a class=\"lite-youtube-fallback\" href=\"https://www.youtube.com/watch?v=guJLfqTFfIw\">Watch on YouTube</a>\n        </lite-youtube>\n        <script type=\"application/ld+json\">\n          {\n            \"@context\": \"https://schema.org/\",\n            \"@type\": \"VideoObject\",\n            \"name\": {{ video.title | json }},\n            \"description\": {{ video.description | strip_html | json }},\n            \"thumbnailUrl\": \"https://img.youtube.com/vi/{{ params[1] }}/3.jpg\",\n            \"uploadDate\": \"{{ video.upload_date | date: '%FT%H:%M:%SZ' }}\",\n            \"duration\": \"PT{% if video.duration_hours %}{{ video.duration_hours }}H{% endif %}{% if video.duration_minutes %}{{ video.duration_minutes }}M{% endif %}{{ video.duration_seconds }}S\",\n            \"embedUrl\": \"{{ video.product_video_url }}\"\n          }\n        </script>\n      {% elsif video.source == 'Vimeo' %}\n        {% assign params = video.product_video_url | split: 'vimeo.com/' %}\n        <iframe\n          src=\"https://player.vimeo.com/video/{{params[1]}}\"\n          frameborder=\"0\"\n          webkitAllowFullScreen\n          mozallowfullscreen\n          allowFullScreen\n        ></iframe>\n      {% else %}\n        <iframe src=\"{{video.product_video_url}}\" style=\"border:0\"></iframe>\n      {% endif %}\n    </div>\n  </div>\n{% endif %}",
