inTime ? 1 : 2));\n    cutoff += inTime ? 0 : 24;\n\n    const todayWeek = todayDate.getDay();\n    const nextDayWeek = nextDay.getDay();\n    const lastDays = deliveryDays.slice(-2).sort((a, b) => b - a);\n\n    if (!deliveryDays.includes(nextDayWeek) || nextDayWeek == deliveryDays[0]) {\n      let targetDay = ((inTime && todayWeek == lastDays[0]) || (todayWeek != lastDays[0] && !inTime && lastDays.includes(todayWeek))) ? deliveryDays[0] : deliveryDays[1];\n\n      do {\n        cutoff += 24;\n        nextDay.setDate(nextDay.getDate() + 1);\n      } while (nextDay.getDay() != targetDay);\n\n      if (lastDays.includes(todayWeek) && targetDay == deliveryDays[0]) {\n        if (todayWeek == lastDays[0] && inTime ||\n          todayWeek == lastDays[1] && !inTime) {\n          cutoff -= 24 * lastDays.length;\n        }\n      }\n    }\n\n    if (!disableSaturday && nextDay.getDay() == 6) {\n      shippingMethod = 'Saturday Delivery';\n    }\n\n    document.querySelector('#deliveryDate').innerHTML = nextDay.toLocaleDateString('en-GB', dateOptions);\n    document.querySelector('#deliveryTime').innerHTML = `${cutoff - (mins > 0 ? hours + 1 : hours)}h ${\n      mins > 0 ? 60 - mins : 0\n    }m`;\n    document.querySelector('#shippingMethod').innerHTML = shippingMethod;\n  }\n</script>\n{% endif %}\n{% endunless %}"
